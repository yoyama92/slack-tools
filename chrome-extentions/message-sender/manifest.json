{
  "name": "slack-message-sender",
  "description": "",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_icon": {
      "16": "icon.png"
    },
    "default_popup": "popup.html"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+H",
        "mac": "MacCtrl+Shift+H"
      },
      "description": "Run extension."
    }
  },
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "options_page": "options.html"
}